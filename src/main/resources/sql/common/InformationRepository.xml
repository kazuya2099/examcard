<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.examcard.repository.common.InformationRepository">

	<select id="selectByDate" parameterType="Date" resultType="com.examcard.repository.common.Information">
	<![CDATA[
		select
			id
			,message
			,start_date
			,end_date
		from
			information
		where
			start_date <= #{systemDate}
			and #{systemDate} <= end_date
	]]>
	</select>
</mapper>